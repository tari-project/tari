[
  {
    "name": "linux-x86_64",
    "runs-on": "ubuntu-20.04",
    "rust": "nightly-2024-02-01",
    "target": "x86_64-unknown-linux-gnu",
    "cross": false,
    "target_cpu": "x86-64",
    "target_bins": "--bin minotari_node --bin minotari_console_wallet --bin minotari_merge_mining_proxy --bin minotari_miner",
    "features": "default, safe"
  },
  {
    "name": "linux-arm64",
    "runs-on": "ubuntu-latest",
    "rust": "stable",
    "target": "aarch64-unknown-linux-gnu",
    "cross": true,
    "target_cpu": "generic",
    "features": "safe",
    "target_bins": "--bin minotari_node --bin minotari_console_wallet --bin minotari_merge_mining_proxy --bin minotari_miner",
    "flags": "--workspace --exclude minotari_mining_helper_ffi --exclude tari_integration_tests"
  },
  {
    "name": "macos-x86_64",
    "runs-on": "macos-11",
    "rust": "stable",
    "target": "x86_64-apple-darwin",
    "cross": false,
    "target_cpu": "x86-64",
    "target_bins": "--bin minotari_node --bin minotari_console_wallet --bin minotari_merge_mining_proxy --bin minotari_miner",
    "features": "default, safe"
  },
  {
    "name": "macos-arm64",
    "runs-on": "macos-12",
    "rust": "stable",
    "target": "aarch64-apple-darwin",
    "cross": false,
    "target_cpu": "generic",
    "target_bins": "--bin minotari_node --bin minotari_console_wallet --bin minotari_merge_mining_proxy --bin minotari_miner",
    "features": "default, safe",
    "build_enabled": true
  },
  {
    "name": "windows-x64",
    "runs-on": "windows-2019",
    "rust": "stable",
    "target": "x86_64-pc-windows-msvc",
    "cross": false,
    "target_cpu": "x86-64",
    "features": "safe",
    "target_bins": "--bin minotari_node --bin minotari_console_wallet --bin minotari_merge_mining_proxy --bin minotari_miner",
    "flags": "--workspace --exclude tari_libtor"
  },
  {
    "name": "windows-arm64",
    "runs-on": "windows-latest",
    "rust": "stable",
    "target": "aarch64-pc-windows-msvc",
    "cross": false,
    "target_cpu": "generic",
    "features": "safe",
    "target_bins": "--bin minotari_node --bin minotari_console_wallet --bin minotari_merge_mining_proxy --bin minotari_miner",
    "build_enabled": false
  }
]
